{
  "name": "sap-cap-sdm-plugin",
  "version": "0.0.1",
  "description": "An SAP CAP plugin designed for effortless integration with SAP Document Management Service. ",
  "author": {
    "name": "Vin√≠cius Barrionuevo",
    "url": "https://github.com/vneecious"
  },
  "license": "MIT",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/vneecious/sap-cap-sdm-plugin"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "main": "cds-plugin.js",
  "files": [
    "lib"
  ],
  "keywords": [
    "sap",
    "cap",
    "dms",
    "sdm",
    "plugin"
  ],
  "scripts": {
    "lint": "npx eslint .",
    "test": "npx jest --silent"
  },
  "peerDependencies": {
    "@sap/cds": "^7"
  },
  "dependencies": {
    "sap-cloud-cmis-client": "^1.3.1"
  },
  "devDependencies": {
    "eslint": "^8.50.0",
    "jest": "^29.7.0",
    "semantic-release": "^22.0.5"
  },
  "plugins": [
    [
      "@semantic-release/commit-analyzer",
      {
        "preset": "conventionalcommits",
        "releaseRules": [
          {
            "type": "build",
            "scope": "deps",
            "release": "patch"
          }
        ]
      }
    ],
    [
      "@semantic-release/release-notes-generator",
      {
        "preset": "conventionalcommits",
        "presetConfig": {
          "types": [
            {
              "type": "feat",
              "section": "Features"
            },
            {
              "type": "fix",
              "section": "Bug Fixes"
            },
            {
              "type": "build",
              "section": "Dependencies and Other Build Updates",
              "hidden": false
            }
          ]
        }
      }
    ],
    "@semantic-release/npm",
    "@semantic-release/github",
    [
      "@semantic-release/git",
      {
        "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
      }
    ]
  ]
}